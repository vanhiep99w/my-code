<?xml version="1.0" encoding="UTF-8"?>
<urlrewrite>		
	<rule>
		<from>/trang-chu</from>
		<to>/public/index</to>
	</rule>	
	<rule>
		<from>/danh-muc/(.*)-([0-9]+)/page-([0-9]+)</from>
		<to>/public/cat?id=$2&amp;page=$3</to>
	</rule>	
	<rule>
		<from>/danh-muc/(.*)-([0-9]+)</from>
		<to>/public/cat?id=$2</to>
	</rule>		
	<rule>
		<from>/tin-tuc/(.*)-([0-9]+)</from>
